{
  "name": "university-bot",
  "version": "1.0.0",
  "description": "University bot app",
  "main": "index.js",
  "engines": {
    "node": "12.17.0",
    "npm": "6.14.8"
  },
  "scripts": {
    "start": "node index.js",
    "backend": "nodemon index.js",
    "frontend": "npm run start --prefix frontend",
    "dev": "concurrently \"npm run backend\"  \"npm run start --prefix frontend\"",
    "heroku-postbuild": "cd frontend && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "author": "rameezyatoo",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dialogflow": "^1.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "google-oauth-jwt": "^0.2.0",
    "install": "^0.13.0",
    "mongoose": "^5.12.5",
    "npm": "^7.10.0"
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7"
  }
}
